<% if (pagination == 2){ %>
  <% page.posts.each(function(post){ %>
    <%- partial('article', {post: post, index: true}) %>
  <% }) %>
  <% if (page.total > 1){ %>
    <nav id="page-nav">
      <%- paginator({
        prev_text: '&laquo; Prev',
        next_text: 'Next &raquo;'
      }) %>
    </nav>
  <% } %>
<% } else if (pagination == 4) { %>
<article class="article article-type-post" itemscope itemprop="blogPost">
  <div class="article-inner ">
    <header class="article-header">
      <h1 itemprop="name">
        <a class="article-title" href="" itemprop="url">Archive</a>
      </h1>
    </header>
    <footer class="article-footer"></footer>
      <div class="widget-wrap">
  </div>
    <div class="article-entry article-index archive" itemprop="articleBody">
    <% var this_year; %>
      <% page.posts.each(function(item) { %>
        <% var year = item.date.year(); %>
        <% if (this_year != year || !this_year){ %>
        <% if(this_year){ %>
          </div></ul>
        <% } %>
      <div class="archive-item"><div id="<%= year %>" class="archive-year"><%= year %></div>
      <ul>
      <% this_year = year; } %>
      <li>
          <time datetime="<%= item.date.toDate().toISOString() %>"><%= item.date.format("MMM-Do") %></time>
          <a class="archive-article-title" href="<%- config.root %><%- item.path %>"><%= item.title %></a>
          <div class="archive-tag">
          <%- list_tags(item.tags, {
            show_count: false,
            class: 'article-tag'
          }) %>
          </div>
      </li>
      <% }); %>
    </div>
  </div>
</article>
<% if (page.total > 1){ %>
    <nav id="page-nav">
      <%- paginator({
        prev_text: '&laquo; Prev',
        next_text: 'Next &raquo;'
      }) %>
    </nav>
  <% } %>
<% } else { %>
  <% var last; %>
  <% page.posts.each(function(post, i){ %>
    <% var year = post.date.year(); %>
    <% if (last != year){ %>
      <% if (last != null){ %>
        </div></section>
      <% } %>
      <% last = year; %>
      <section class="archives-wrap">
        <div class="archive-year-wrap">
          <a href="<%- url_for('archives/' + year) %>" class="archive-year"><%= year %></a>
        </div>
        <div class="archives">
    <% } %>
    <%- partial('archive-post', {post: post, even: i % 2 == 0}) %>
  <% }) %>
  <% if (page.posts.length){ %>
    </div></section>
  <% } %>
<% } %>